{
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}"
        ],
        "myCompilerPath": "${env:ARM_TOOLCHAIN}/bin/arm-none-eabi-gcc.exe"
    },
    "configurations": [
        {
            "name": "Windows",
            "intelliSenseMode": "clang-x64",
            "includePath": [
                "${workspaceFolder}/**"
                "${env:ARM_TOOLCHAIN}/lib/gcc/arm-none-eabi/11.3.1/include/"
            ],
            "defines": [
                "FOO",
                "BAR=100"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}